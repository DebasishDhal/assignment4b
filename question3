#include<iostream>
using namespace std;
int printeven(int,int);
int main()
{
int a,b;
cout<<"enter a range such that lower range is entered first"<<endl;
cin>>a>>b;
printeven(a,b);
return 0;
}
int printeven(int a,int b)
{
if(a%2==0)
{
if(a!=b+1)
{
cout<<a<<endl;
printeven((a+2),b);
}
else 
return 0;
}
else
{
if(a!=b+1)
{
cout<<(a+1)<<endl;
printeven((a+2),b);
}
else 
return 0;
}
}
